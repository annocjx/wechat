			<if test="parentId != null"> parentId = #{parentId}, </if>
			<if test="areaLevel != null"> areaLevel = #{areaLevel}, </if>
			<if test="shortName != null"> shortName = #{shortName}, </if>
			<if test="areaUrl != null"> areaUrl = #{areaUrl}, </if>
		</set>
		where id = #{id}
	</update>
